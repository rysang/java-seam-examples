<ehcache>
	<diskStore path="java.io.tmpdir" />

	<cache name="org.sample.cache.METHOD_CACHE" maxElementsInMemory="300"
		eternal="false" timeToIdleSeconds="500" timeToLiveSeconds="500"
		overflowToDisk="true" />

	<defaultCache maxElementsInMemory="50000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="7200" timeToLiveSeconds="7200"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />

</ehcache>
